local UIS = game:GetService("UserInputService")
local PC, Touchscreen, Controller =  (UIS.MouseEnabled and UIS.KeyboardEnabled), UIS.TouchEnabled, UIS.GamepadEnabled
if not PC and Touchscreen then
loadstring(game:HttpGet("https://raw.githubusercontent.com/EasyDestroyer/EasyDestroyer.gg/main/mobile"))()
  
  local title = "Device Detected"
  local text = "You are using a mobile device, loading UI!"
  local time = 5
  NotifyUser(title, text, time)
end
